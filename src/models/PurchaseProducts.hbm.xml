<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="models.PurchaseProducts" table="PurchaseProducts" schema="internet-shop">
        <composite-id mapped="true" class="models.PurchaseProductsPK">
            <key-property name="purchaseId">
                <column name="purchase_id" sql-type="bigint(20)"/>
            </key-property>
            <key-property name="productId">
                <column name="product_id" sql-type="bigint(20)"/>
            </key-property>
        </composite-id>
        <property name="num">
            <column name="num" sql-type="int(11)"/>
        </property>
        <many-to-one name="purchaseByPurchaseId" class="models.Purchase">
            <column name="purchase_id"/>
        </many-to-one>
        <many-to-one name="productByProductId" class="models.Product">
            <column name="product_id"/>
        </many-to-one>
    </class>
</hibernate-mapping>